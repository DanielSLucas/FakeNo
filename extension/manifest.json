{
  "manifest_version": 3,
  "name": "FakeNo",
  "description": "Fake news detection extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {    
    "default_icon": "images/fakeNoIcon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/"
  ]
}